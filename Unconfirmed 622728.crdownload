<script>
function select_mod1_dealer()
{
    var personal_details_pop = $('#personal_details_pop').val();
	var new_locatorinput = $('#new_select_locatorinput').val(); 
	var new_usr_city = $('#new_usr_city').val(); 
	var new_dlrid = $('#new_dlrid').val(); 
	var new_dlrid_state = $('#new_dlrid_state').val(); 
	var new_dlrid_city = $('#new_dlrid_city').val(); 
	var new_dlrid_uid = $('#new_dlrid_uid').val();
	var new_dlrid_name = $('#new_dlrid_name').val();
	//console.log(new_usr_city);
    var new_locality = $('#locality').val();
		$.ajax({
		url : 'https://www.findmeasuv.in/locator/new_submit',
		type : 'post',
		//data : {dlrid:new_dlrid,dlr_states:new_dlrid_state,dlr_city:new_dlrid_city,dlr_uid:new_dlrid_uid,city_auto:new_usr_city,dlr_actual_name:new_dlrid_name},
		data : {dlrid:new_dlrid,city_auto:new_usr_city,new_locality:new_locality},
		dataType : 'json',
		success : function (response) {
		     //alert(response);
			if(response.msg == 'success')
			{   
				if (typeof(CURRENT_LOCATION)!='undefined' && CURRENT_LOCATION=='payment') {
					$.ajax({
						url : 'https://www.findmeasuv.in/payment/reset_car_details',
						dataType : 'json',
						success : function (response) {
							if(response.msg == 'success')
							{   
								if(personal_details_pop =="T"){
							     //$('#quote_type').val('book_test_drive');
							     window.location.href = response.redirect+"?&book_test_drive_value";
							 }else if(personal_details_pop =="S"){
							     //$('#quote_type').val('book_test_drive');
							     window.location.href = response.redirect+"?&sent_a_quote_value";
							 }else if(personal_details_pop =="C"){
							     //$('#quote_type').val('book_test_drive');
							     window.location.href = response.redirect+"?&requst_call_back";
							 }else{
								window.location.href = response.redirect;
							
							 }
							}
						}
					});
				}
				/*else window.location.href = window.location.href;*/
				else location.reload();
			}
		}
	});
	
	
}

</script>
<script>

   $(function(){
       //dealer_selection_popup = $.popup.create("select_new_dealer_popup", {"center": true, "background": true, "template": $("#basic_template")});
       $("#new_select_locatorinput").bind("click", function() {
          
        $("#new_select_locatorinput").autocomplete({
  	      
  		delay:null,
  		minLength: 1,
  		autoFocus: true,
  		source:"https://www.findmeasuv.in/locator/new_get_city_list?locationType=city",
  		focus: function(event, ui){ 
  		 
			$("#new_locatorinput").val()
  			return false;
  		},
  		
  		open: function(event, ui){
  		
  			//$(".ui-autocomplete").append("<li class='list-group-item p_details ui-menu-item' role='presentation' ><a href='javascript:void(0);' class='no-border' id='shw_all_dlr'><i class='fa fa-map-marker' aria-hidden='true'></i>Show all dealers</a></li>");	
  		},
  		select: function(event, ui){
			if (ui.item.dlrid=='_ALL') {
				$("#new_select_locatorinput").val('');
				$('#shw_all_dlr').modal('show');
			} else {
				$("#new_select_locatorinput").val(ui.item.loc+', '+ui.item.abbr+', '+ui.item.name);
				$("#new_usr_city").val(ui.item.loc);
				$("#new_dlrid").val(ui.item.dlrid);
				$("#new_dlrid_state").val(ui.item.d_states);
				$("#new_dlrid_city").val(ui.item.d_city);
				$("#new_dlrid_uid").val(ui.item.d_uid);
				$("#new_dlrid_name").val(ui.item.name);
				$("#new_state_abbr").val(ui.item.abbr);
				$("#locality").val(ui.item.locality);
				if($('#sel_check').is(':checked') == true ) {
					$('#display_sel_error').html('');
					select_mod1_dealer();
				} else {
					$('#display_sel_error').html('<p style="color: red !important;font-size:10px !important;">To proceed further kindly accept the Terms & Conditions above.</p>');
				}
			}
			return false;
  		}
  	}).data('ui-autocomplete')._renderItem = function(ul, item){
		if (item.dlrid=='_ALL') {var inner_html = ' Show All Dealer ';}
		else{ if (item.locality != '') { var inner_html = '  '+item.name+','+item.locality+', '+item.d_city+', '+item.d_states;}
									else{ var inner_html = '  '+item.name+', '+item.d_city+', '+item.d_states;}
			}
		/*else var inner_html = '  '+item.name+', '+item.loc+', '+item.d_states;*/
  		return $('<li class="list-group-item p_details pop-dlr" role="presentation" >').append("<a href='javascript:void(0);' class='no-border' ><i class='fa fa-map-marker' aria-hidden='true'></i>"+inner_html+ "</a>").appendTo(ul);
  	}
    
    });
     
return false;
   });  
   
   $(document).ready(function() {
        $("#sel_check").click(function(event) {
            if($('#sel_check').is(':checked') == true ) {
                var loc_val=$('#new_select_locatorinput').val();
                if(loc_val!=''){
                    select_mod1_dealer();    
                }
            }
        });
    });
   
</script>    
    
